#!/usr/bin/env ruby
# sudo perf record /home/k0kubun/.rbenv/versions/2.4.1/bin/ruby bin/llrb bin/scripts/bm_app_fib.rb

require 'bundler/setup'
require 'llrb'

script = File.read(ARGV.first)
native = Class.new
native.instance_eval("def self.script; #{script}; end")
LLRB::JIT.precompile(native, :script)

native.script
